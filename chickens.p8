  state.update=update_tutorial
  state.draw=draw_tutorial

  worm={}
  worm.tmr=0
  local loc={}
  loc.x=60
  loc.y=50
  worm.loc=loc
 end
 for x=1,3 do
  ss_chickens[x].x-=1
  if(ss_chickens[x].x<=-24) then ss_chickens[x].x=128 end
 end
 chicken.tmr+=1
 if(chicken.tmr==60) then
  chicken.tmr=0
 end
end

