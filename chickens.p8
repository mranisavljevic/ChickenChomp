__lua__
--chickens
--by creeperspeak 2017

state={}
mode=0
chicken={}
last_dir=2
friction=0.1
spd=0.2

coop={}
worm={}

shdw={}

ss_chickens={}

highscore=0

function _init()
	cls()
	pal(8,0)
	highscore=max(dget(0),0)
 init()
end

function init()
 state.draw=draw_startscreen
 state.update=update_startscreen
 chicken.selection=0
 chicken.x=60
 chicken.y=60
 chicken.acc={}
 chicken.acc.x=0
 chicken.acc.y=0
 chicken.hngr=0
 chicken.tmr=0
 chicken.tot=0
 chicken.sprt=2
 chicken.running=0
 make_worms()
 set_shadow()

 local c1={}
 local c2={}
 local c3={}
 c1.s=1
 c1.x=128
 c2.s=4
 c2.x=148
 c3.s=7
 c3.x=168
 ss_chickens={c1,c2,c3}
end

function _update()
	state.update()
end

function _draw()
	state.draw()
end

function set_shadow()
	shdw[4]=2
	shdw[5]=1
	shdw[6]=13
	shdw[14]=4
end

function draw_startscreen()
 map(20,0,0,0,128/8,128/8)
 print("press — to start",30,100,12)
 print("by creeperspeak",32,112,12)
 for x=1,3 do
  local c=ss_chickens[x]
  spr(c.s,c.x,80)

  if(chicken.tmr%2==1) then
   pset(c.x+5,86,9)
   pset(c.x+5,87,9)
   pset(c.x+4,87,9)
  else
   pset(c.x+4,86,9)
   pset(c.x+4,87,9)
   pset(c.x+3,87,9)
  end
 end
end

function update_startscreen()
